{
  "extends": "tslint:latest",
  "rules": {
    "semicolon": [true, "never"],
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "trailing-comma": [true, "never"],
    "arrow-parens": [true, "ban-single-arg-parens"],
    "no-console": false,
    "max-line-length": false,
    "ordered-imports": false
  }
}